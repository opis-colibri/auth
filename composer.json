{
    "name": "opis-colibri/auth",
    "type": "opis-colibri-module",
    "description": "A module that deals with user authentication",
    "keywords": ["auth", "module", "opis colibri"],
    "homepage": "https://opiscolibri.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        },
        {
            "name": "Sorin Sarca",
            "email": "sarca_sorin@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "opis-colibri/core": "2020.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Opis\\Colibri\\Module\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\Colibri\\Module\\Auth\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "module": {
            "tile": "Auth module",
            "collector": "Opis\\Colibri\\Module\\Auth\\Collector",
            "installer": "Opis\\Colibri\\Module\\AUth\\Installer"
        },
        "branch-alias": {
            "dev-master": "2020.x-dev"
        }
    }
}